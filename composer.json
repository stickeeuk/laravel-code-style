{
  "name": "stickee/laravel-code-style",
  "authors": [
    {
      "name": "Paul Tibbetts",
      "email": "paul.tibbetts@stickee.co.uk"
    },
    {
      "name": "Rehan Aslam",
      "email": "rehan.aslam@stickee.co.uk"
    }
  ],
  "require": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "sensiolabs/security-checker": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Stickee\\LaravelCodeStyle\\": "src"
    },
    "classmap": [
      "src"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Stickee\\LaravelCodeStyle\\StickeeServiceProvider"
      ]
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  }
}
